{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "exit",
          "background": "#2e9599",
          "foreground": "#000000",
          "properties": {
            "always_enabled": true,
            "display_exit_code": true
          },
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "template": " \uE23A {{ if gt .Code 0 }}\uF00D {{ .Meaning }}{{ else }}\uF42E{{ end }} "
        },
        {
          "type": "executiontime",
          "background": "#00ff95",
          "foreground": "#000000",
          "properties": {
            "threshold": 500,
            "always_enabled": true,
            "style": "austin"
          },
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "template": " \uF250 {{ .FormattedMs }} "
        },
        {
          "type": "time",
          "background": "#ff00f2",
          "foreground": "#000000",
          "properties": {
            "time_format": "\uF43A 15:04:05"
          },
          "style": "diamond",
          "powerline_symbol": "\uE0B0",
          "trailing_diamond": "\uE0B4"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "foreground": "#21c7c7",
          "style": "plain",
          "template": "â€Ž"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "session",
          "foreground": "#000000",
          "background": "#ff0000",
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B4",
          "template": " \uF2C0 {{ .UserName }}{{ if .SSHSession }} \uE0BD  \uEB3A SSH{{ end }} "
        },
        {
          "type": "git",
          "background": "#95ffa4",
          "foreground": "#193549",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}"
          ],
          "properties": {
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B4",
          "template": " {{ .UpstreamIcon }} {{ .HEAD }} {{ .BranchStatus }} {{ if .Working.Changed }}\uF044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uF046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uF692 {{ .StashCount }}{{ end }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "path",
          "background": "#00ff00",
          "foreground": "#000000",
          "properties": {
            "folder_separator_icon": " \uE0B1 ",
            "style": "agnoster_full"
          },
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B4",
          "template": " \uE5FF {{ .Path }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "python",
          "background": "#FF6471",
          "foreground": "#ffffff",
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B4",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} "
        },
        {
          "type": "root",
          "background": "#ff4444",
          "foreground": "#100e23",
          "properties": {
            "root_icon": "\uF0E7"
          },
          "style": "diamond",
          "leading_diamond": "\uE0B6",
          "trailing_diamond": "\uE0B4",
          "template": " \uF0E7 "
        },
        {
          "type": "text",
          "foreground": "#21c7c7",
          "style": "plain",
          "template": " \uE285"
        }
      ]
    }
  ],
  "final_space": true,
  "version": 2
}