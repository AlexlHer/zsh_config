{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "status",
          "background": "#2e9599",
          "foreground": "#000000",
          "properties": {
            "always_enabled": true,
            "status_template": "{{ if gt .Code 0 }}<f>󰋔 </f>\uF00D {{ reason .Code }}{{ else }}󰣐 \uF42E{{ end }}",
            "status_separator": " | "
          },
          "style": "powerline",
          "powerline_symbol": "\uE0B0"
        },
        {
          "type": "executiontime",
          "background": "#00c3ca",
          "foreground": "#000000",
          "properties": {
            "threshold": 500,
            "always_enabled": true,
            "style": "austin"
          },
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "template": " \uF250 {{ .FormattedMs }} "
        },
        {
          "type": "time",
          "background": "#00f7ff",
          "foreground": "#000000",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "template": " \uF43A {{ .CurrentDate | date .Format }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "#000000",
          "background": "#000000",
          "template": "<transparent,transparent></>"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "session",
          "foreground": "#000000",
          "background": "#ff4800",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "template": "{{ if .Root }} \uF09C {{ else }} \uF2C0 {{ end }}{{ .UserName }}{{ if .SSHSession }} \uE0BD \uEB3A SSH{{ end }} "
        },
        {
          "type": "session",
          "foreground": "#000000",
          "background": "#ff9900",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "template": " {{ .Segments.Os.Icon }} {{ .HostName }} "
        },
        {
          "type": "root",
          "background": "#ff0000",
          "foreground": "#100e23",
          "properties": {
            "root_icon": "\uF0E7"
          },
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "template": " \uF0E7 "
        },
        {
          "type": "python",
          "background": "#FF6471",
          "foreground": "#ffffff",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} "
        },
        {
          "type": "git",
          "background": "#95ffa4",
          "foreground": "#193549",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}"
          ],
          "properties": {
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "template": " {{ .UpstreamIcon }} {{ .HEAD }} {{ .BranchStatus }} {{ if .Working.Changed }}\uF044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uF046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uF692 {{ .StashCount }}{{ end }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "path",
          "background": "#00ff00",
          "foreground": "#000000",
          "properties": {
            "folder_separator_icon": " \uE0B1 ",
            "style": "agnoster_full"
          },
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "template": " \uE5FF {{ .Path }} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "foreground": "#21c7c7",
          "style": "plain",
          "template": "\uE285"
        }
      ]
    }
  ],
  "console_title_template": "{{.UserName}}@{{.HostName}} {{.Shell}} in {{.PWD}}",
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "<transparent,#2e9599>\uE0B0 {{ .Segments.Status.String }} </><#2e9599,#00c3ca>\uE0B0</><transparent,#00c3ca> \uF250 {{ .Segments.Executiontime.FormattedMs }} </><#00c3ca,#00f7ff>\uE0B0</><transparent,#00f7ff> \uF43A {{ .Segments.Time.CurrentDate | date .Segments.Time.Format }} </><#00f7ff,transparent>\uE0B0</> \n\n<#21c7c7,transparent>\uE285</> "
  },
  "final_space": true,
  "version": 2
}
